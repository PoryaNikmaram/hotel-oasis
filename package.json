{
  "name": "the-wild-oasis",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Professional Wild Oasis booking application built with Next.js and Supabase",
  "author": "Your Team",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "react",
    "supabase",
    "booking",
    "hospitality"
  ],
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "preview": "npm run build && npm run start",
    "lint": "eslint . --cache --cache-location .eslintcache",
    "lint:fix": "eslint . --fix --cache --cache-location .eslintcache",
    "lint:check": "eslint . --cache --cache-location .eslintcache",
    "format": "prettier --write . --cache",
    "format:check": "prettier --check . --cache",
    "typecheck": "tsc --noEmit || echo 'TypeScript not configured'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2 --passWithNoTests",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:headed": "playwright test --headed",
    "prepare": "husky",
    "pre-commit": "lint-staged",
    "validate": "npm run lint:check && npm run format:check && npm run typecheck",
    "validate:full": "npm run validate && npm run test:ci",
    "clean": "rimraf .next out build dist coverage playwright-report test-results .eslintcache",
    "clean:all": "npm run clean && rimraf node_modules package-lock.json",
    "reinstall": "npm run clean:all && npm install",
    "analyze": "ANALYZE=true npm run build",
    "deps:check": "npx npm-check-updates",
    "deps:update": "npx npm-check-updates -u && npm install",
    "audit:fix": "npm audit fix"
  },
  "lint-staged": {
    "*.{js,jsx,mjs}": [
      "eslint --fix --cache",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml,css,scss}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@supabase/supabase-js": "^2.49.4",
    "date-fns": "^4.1.0",
    "next": "15.4.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sharp": "^0.34.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.27.0",
    "@next/bundle-analyzer": "^15.4.6",
    "@playwright/test": "^1.49.0",
    "@tailwindcss/forms": "^0.5.9",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.33.0",
    "eslint-config-next": "^15.4.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.10.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-playwright": "^2.1.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "lint-staged": "^16.1.2",
    "postcss": "^8.4.49",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "browserslist": {
    "production": [
      ">0.3%",
      "not ie 11",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
